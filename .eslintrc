{
  "parser": "babel-eslint",
  "sourceType": "module",
  "ecmaFeatures": {
    "arrowFunctions": true,
    "blockBindings": true,
    "destructuring": true,
    "regexYFlag": true,
    "regexUFlag": true,
    "templateStrings": true,
    "binaryLiterals": true,
    "octalLiterals": true,
    "unicodeCodePointEscapes": true,
    "defaultParams": true,
    "restParams": true,
    "forOf": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "objectLiteralDuplicateProperties": true,
    "generators": true,
    "spread": true,
    "classes": true,
    "modules": true,
    "jsx": true,
    "globalReturn": true
  },
  "rules": {
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-dupe-keys": 2,
    "no-empty": 2,
    "no-extra-boolean-cast": 0,
    "no-extra-semi": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-regex-spaces": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "block-scoped-var": 0,
    // Uncomment when tracking down good refactor candidates
    // "complexity": [1, 2],
    "default-case": 2,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-caller": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-implied-eval": 2,
    "no-loop-func": 2,
    "no-new-wrappers": 2,
    "no-return-assign": 0,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "wrap-iife": [2, "outside"],
    "no-use-before-define": [2, "nofunc"],
    "indent": [2, 2],
    //"brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "comma-spacing": [2, {"before": false, "after": true}],
    "consistent-this": [0, "self"],
    "eol-last": 2,
    "max-nested-callbacks": [2, 2],
    "no-mixed-spaces-and-tabs": 2,
    "semi-spacing": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "operator-assignment": [2, "always"],
    "quote-props": [2, "as-needed"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "keyword-spacing": [2, { "before": true, "after": true }],
    "spaced-comment": 0,
    "no-var": 2,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "new-cap": 0, // new-cap throws on Immutable.Map, etc
    "no-underscore-dangle": 0,
    "curly": 0,
    "yoda": 0,
    "no-multi-spaces": 0,
    "camelcase": 0,
    "key-spacing": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "globals": {
    "angular": false,
    "_": false
  }
}
